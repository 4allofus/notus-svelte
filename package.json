{
  "name": "tailwind-webapp-svelte",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -s",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run dev"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "14.0.0",
    "@rollup/plugin-node-resolve": "8.0.0",
    "rollup": "2.3.4",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-svelte": "6.0.0",
    "rollup-plugin-terser": "7.0.0",
    "svelte": "3.0.0"
  },
  "dependencies": {
    "@tailwindcss/custom-forms": "0.2.1",
    "sirv-cli": "1.0.0",
    "svelte-routing": "1.4.2",
    "tailwindcss": "1.8.5"
  }
}
